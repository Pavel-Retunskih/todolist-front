{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/app/*": [
        "./src/app/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/features/*": [
        "./src/features/*"
      ],
      "@/entities/*": [
        "./src/entities/*"
      ],
      "@/widget/*": [
        "./src/widget/*"
      ],
      "@/shared/*": [
        "./src/shared/*"
      ]
    }
  }
}
